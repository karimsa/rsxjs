{
  "name": "rsxjs",
  "version": "0.0.0",
  "description": "Resilience Extensions for JS.",
  "main": "dist/index.js",
  "scripts": {
    "test": "ava -v $(node scripts/test-files.js)",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karimsa/rsxjs.git"
  },
  "keywords": [
    "resilience",
    "patterns",
    "data",
    "structures",
    "extensions",
    "chaos"
  ],
  "author": "Karim Alibhai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karimsa/rsxjs/issues"
  },
  "homepage": "https://github.com/karimsa/rsxjs#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@types/debug": "0.0.30",
    "@types/node": "^10.5.2",
    "ava": "^1.0.0-beta.3",
    "bluebird": "^3.5.1",
    "lolex": "^2.7.1",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-typescript2": "^0.16.1",
    "sinon": "^6.1.4",
    "typescript": "^2.9.2"
  },
  "ava": {
    "timeout": "4s",
    "files": [
      "tests/**/test-*.js"
    ],
    "compileEnhancements": true,
    "require": [
      "@babel/register",
      "@babel/polyfill"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "only": [
          "tests/**/*.js"
        ],
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "dependencies": {
    "@foko/type-check": "0.0.4",
    "debug": "^3.1.0"
  }
}
