{
  "name": "rsxjs",
  "version": "0.5.2",
  "description": "Resilience Extensions for JS.",
  "main": "dist/index.js",
  "scripts": {
    "test": "node -r source-map-support/register node_modules/.bin/nyc node --expose_gc node_modules/.bin/ava -v",
    "build": "rollup -c",
    "clean": "rm -rf dist",
    "coverage": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/karimsa/rsxjs.git"
  },
  "keywords": [
    "resilience",
    "patterns",
    "data",
    "structures",
    "extensions",
    "chaos"
  ],
  "author": "Karim Alibhai",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/karimsa/rsxjs/issues"
  },
  "homepage": "https://github.com/karimsa/rsxjs#readme",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.54",
    "@babel/polyfill": "^7.0.0-beta.54",
    "@babel/preset-env": "^7.0.0-beta.54",
    "@babel/register": "^7.0.0-beta.54",
    "@types/debug": "0.0.30",
    "@types/ioredis": "^4.0.1",
    "@types/node": "^10.5.2",
    "@types/weak": "^1.0.0",
    "@types/uuid": "^3.4.4",
    "ava": "^1.0.0-beta.3",
    "bluebird": "^3.5.1",
    "codecov": "^3.0.4",
    "lolex": "^2.7.1",
    "nyc": "^12.0.2",
    "rollup": "^0.63.4",
    "rollup-plugin-babel": "^4.0.0-beta.7",
    "rollup-plugin-typescript2": "^0.16.1",
    "sinon": "^6.1.4",
    "source-map-support": "^0.5.9",
    "typescript": "^2.9.2"
  },
  "ava": {
    "cache": true,
    "timeout": "10s",
    "files": [
      "tests/**/test-*.js"
    ],
    "require": [
      "@babel/polyfill"
    ],
    "babel": {
      "testOptions": {
        "babelrc": false,
        "include": [
          "tests/**/*.js"
        ],
        "exclude": [
          "tests/integration/cache/test-concurrent.js"
        ],
        "presets": [
          "@babel/preset-env"
        ]
      }
    }
  },
  "optionalDependencies": {
    "ioredis": "^4.0.0"
  },
  "dependencies": {
    "@foko/type-check": "0.0.4",
    "debug": "^3.1.0",
    "weak": "^1.0.1",
    "uuid": "^3.3.2"
  }
}
